		resultType="java.lang.String"
		>
		select DISTINCT
		(sp_code) spCode
		FROM		
			tbl_sales_task
		WHERE 1 = 1
		<if test="month != null">
			and month=#{month}
		</if>
		<if test="regionalManagerLeader != null">
			and regional_manager_leader=#{regionalManagerLeader}
		</if>
		<if test="regionalManager != null">
			and regional_manager like '%${regionalManager}%'
		</if>
	</select>

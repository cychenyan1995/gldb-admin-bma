	<select id="selectAllSalesHistory"
		parameterType="com.glsx.oms.bigdata.admin.bma.salesHistory.model.SalesHistory"
		resultType="com.glsx.oms.bigdata.admin.bma.salesHistory.model.SalesHistory">
		SELECT id id,
		month month,
		area area,
		biz_type bizType,
		sp_code spCode,
		sp_name spName,
		sales_gl_dvd salesGlDvd,
		sales_gl_gps salesGlGps,
		sales_yunjing salesYunjing,
		sales_didihu_service salesDidihuService,
		sales_wireless_charge salesWirelessCharge,
		sales_rearview
		salesRearview,
		sales_function_rearview salesFunctionRearview
		FROM
		tbl_sales_statement_monthly
		WHERE 1 = 1
		<if test="month != null">
			and month=#{month}
		</if>
		<if test="spCode != null and spCode != ''">
			and sp_code=#{spCode}
		</if>
		<if test="spName != null and spName != ''">
			and sp_name=#{spName}
		</if>
	</select>

	<select id="selectSpSalesHistory"
		parameterType="com.glsx.oms.bigdata.admin.bma.salesHistory.model.SalesHistory"
		resultType="com.glsx.oms.bigdata.admin.bma.salesHistory.model.SalesHistory">
		SELECT m.id id,
		m.month month,
		t.month tmonth,
		m.area area,
		m.biz_type
		bizType,
		t.regional_manager_leader regionalManagerLeader,
		t.regional_manager regionalManager,
		m.sp_code spCode,
		m.sp_name spName,
		t.tasknum_dvd tasknumDvd,
		sum(m.sales_gl_dvd) salesGlDvd,
		CONCAT(CAST(round((sum(m.sales_gl_dvd)/t.tasknum_dvd)*100,2) AS
		CHAR),'%') salesGlDvdRate,
		t.tasknum_gps tasknumGps,
		sum(m.sales_gl_gps) salesGlGps,
		CONCAT(CAST(round((sum(m.sales_gl_gps)/t.tasknum_gps)*100,2) AS
		CHAR),'%') salesGlGpsRate,
		t.tasknum_yunjing tasknumYunjing,
		sum(m.sales_yunjing) salesYunjing,
		CONCAT(CAST(round((sum(m.sales_yunjing)/t.tasknum_yunjing)*100,2) AS
		CHAR),'%') salesYunjingRate,
		t.tasknum_didihu_service
		tasknumDidihuService,
		sum(m.sales_didihu_service) salesDidihuService,
		CONCAT(CAST(round((sum(m.sales_didihu_service)/t.tasknum_didihu_service)*100,2)
		AS CHAR),'%') salesDidihuServiceRate,
		t.tasknum_wireless_charge
		tasknumWirelessCharge,
		sum(m.sales_wireless_charge)
		salesWirelessCharge,
		CONCAT(CAST(round((sum(m.sales_wireless_charge)/t.tasknum_wireless_charge)*100,2)
		AS CHAR),'%') salesWirelessChargeRate,
		t.tasknum_rearview
		tasknumRearview,
		sum(m.sales_rearview) salesRearview,
		CONCAT(CAST(round((sum(m.sales_rearview)/t.tasknum_rearview)*100,2) AS
		CHAR),'%') salesRearviewRate,
		t.tasknum_function_rearview
		tasknumFunctionRearview,
		sum(m.sales_function_rearview)
		salesFunctionRearview,
		CONCAT(CAST(round((sum(m.sales_function_rearview)/t.tasknum_function_rearview)*100,2)
		AS CHAR),'%') salesFunctionRearviewRate
		FROM
		tbl_sales_statement_monthly m
		LEFT JOIN tbl_sales_task t ON m.sp_code =
		t.sp_code AND m.month = t.month
		WHERE m.month IS NOT NULL AND (m.month
		!= '')
		<if test="month != null">
			and m.month=#{month}
		</if>
		<if test="spCode != null and spCode != ''">
			and m.sp_code=#{spCode}
		</if>
		<if test="spName != null and spName != ''">
			and m.sp_name=#{spName}
		</if>
		<if test="area != null and area != ''">
			and m.area=#{area}
		</if>
		<if test="regionalManagerLeader != null and regionalManagerLeader != ''">
			and t.regional_manager_leader=#{regionalManagerLeader}
		</if>
		<if test="regionalManager != null and regionalManager != ''">
			and t.regional_manager like '%${regionalManager}%'
		</if>

		GROUP BY m.sp_code,m.month
	</select>

